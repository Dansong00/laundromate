# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_API_URL_DEV=http://localhost:8000
NEXT_PUBLIC_API_URL_STAGING=https://your-staging-api.railway.app
NEXT_PUBLIC_API_URL_PRODUCTION=https://your-production-api.railway.app

# Authentication
NEXT_PUBLIC_AUTH_DOMAIN=your-auth-domain.auth0.com
